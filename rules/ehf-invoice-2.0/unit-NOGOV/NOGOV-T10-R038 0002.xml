			<cbc:TaxAmount>0.000</cbc:TaxAmount>
		</cac:TaxTotal>
		<cac:TaxTotal>
			<cbc:TaxAmount>0.0000</cbc:TaxAmount>
		</cac:TaxTotal>
		<cac:TaxTotal>
			<cbc:TaxAmount>0.00000</cbc:TaxAmount>
		</cac:TaxTotal>
		<cac:TaxTotal>
			<cbc:TaxAmount>0.000000</cbc:TaxAmount>
		</cac:TaxTotal>
		<cac:TaxTotal>
			<cbc:TaxAmount>0.0000000</cbc:TaxAmount>
