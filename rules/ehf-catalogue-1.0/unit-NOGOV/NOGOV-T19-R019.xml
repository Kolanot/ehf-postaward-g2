<?xml version="1.0" encoding="UTF-8"?>
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0" configuration="ehf-t19-base-1.0">
	<assert>
		<scope>NOGOV-T19-R019</scope>
	</assert>
	<test>
		<assert>
			<description>Verify only allowed tax categories are used.</description>
			<success>NOGOV-T19-R019</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
			xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">

			<cac:TaxCategory>
				<cbc:ID>AA</cbc:ID>
				<cbc:ID>E</cbc:ID>
				<cbc:ID>H</cbc:ID>
				<cbc:ID>K</cbc:ID>
				<cbc:ID>R</cbc:ID>
				<cbc:ID>S</cbc:ID>
				<cbc:ID>Z</cbc:ID>
			</cac:TaxCategory>

		</Catalogue>
	</test>
	<test>
		<assert>
			<description>Trigger error when non-allowed tax categories are used.</description>
			<error number="2">NOGOV-T19-R019</error>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
			xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">

			<cac:TaxCategory>
				<cbc:ID>AAA</cbc:ID>
				<cbc:ID></cbc:ID>
			</cac:TaxCategory>

		</Catalogue>
	</test>
</testSet>
